<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="processor" class="preprocessor.PreProcessorToUpperImpl"/>

    <bean id="render" class="render.RendererErrImpl" >
        <constructor-arg name="preProcessor" ref="processor"/>
    </bean>

    <bean id="printerWithPrefix" class="printer.PrinterWithPrefixImpl" >
        <property name="prefix" value="prefix"/>
        <constructor-arg name="renderer" ref="render"/>
    </bean>

</beans>